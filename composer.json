{
  "name": "galileo/galileo-setting-bundle",
  "description": "Symfony Framework bundle responsible for adding simple configuration possiblities persisted in external storage mechanims",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "Galileo\\SettingBundle\\": ""
    }
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "5.3.9"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=5.3.9",
    "symfony/framework-bundle": "^2.7 || ^3.0"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "leanphp/phpspec-code-coverage": "^2.1",
    "phpspec/phpspec": "^2.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kamil Ronewicz",
      "email": "galileox86@gmail.com"
    }
  ],
  "minimum-stability": "dev-master"
}
